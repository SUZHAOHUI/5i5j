			<value>com/oio/wawj/orm/mapping/SubsRela.hbm.xml</value>
			<value>com/oio/wawj/orm/mapping/TransferNumberSet.hbm.xml</value>
			<value>com/oio/wawj/orm/mapping/CdrCall.hbm.xml</value>
